{
  "openapi": "3.0.0",

  "info": {
    "title": "API Banco de currículos JobNet",
    "description": "API criada para cadastrar dados de uma tabela no banco de dados",
    "version": "0.0.1"
  },

  "servers": [
    {
      "url": "http://localhost:5000",
      "description": "ambiente de desenvolvimento"
    }
  ],

 
  "paths": {
    "/add": {
      "post": {
        "summary": "Cadastro de candidato",
        "description": "Rota de cadastro de usuário, recebe: nome, cargo, dataNascimento, estadoCivil, genero, cep, endereco, numero, bairro, cidade, complemento, fone, cel, emai, rg, cpf, veiculo, cnh",
        "tags": ["candidato"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "nome": {
                    "type": "string"
                  },
                  "cargo": {
                    "type": "string"
                  },
                  "dataNasc": {
                    "type": "string"
                  },
                  "estadoCivil": {
                    "type": "string"
                  },
                  "sexo": {
                    "type": "string"
                  },
                  "cep": {
                    "type": "number"
                  },
                  "rua": {
                    "type": "string"
                  },
                  "numero": {
                    "type": "number"
                  },
                  "bairro": {
                    "type": "string"
                  },
                  "cidade": {
                    "type": "string"
                  },
                  "complemento": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "celular": {
                    "type": "string"
                  },
                  "tel1": {
                    "type": "string"
                  },
                  "identidade": {
                    "type": "string"
                  },
                  "cpf": {
                    "type": "string"
                  },
                  "habilitacao": {
                    "type": "string"
                  },
                  "carro": {
                    "type": "string"
                  }

                }
              },
              "examples": {
                "Candidato": {
                  "value": {
                    "nome": "João Silva",
                    "cargo": "Dev",
                    "dataNasc": "10/10/1980",
                    "estadoCivil": "Solteiro(a)",
                    "sexo": "outro",
                    "cep": "01001000",
                    "rua": "praca da sé",
                    "numero": "10",
                    "bairro": "centro",
                    "cidade": "São Paulo",
                    "complemento": " ",
                    "email": "11 11111111",
                    "celular": "11 999999999",
                    "tel1": "joao@email.com",
                    "identidade": "000000000",
                    "cpf": "11111111111",
                    "habilitacao": "B",
                    "carro": "sim"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}